<div class = "section group">

    <div class = "smaller-container">

    <p class = "top-space" id="notice"><%= notice %></p>


<p>
  <b>Name:</b>
  <%= @post.name %>
</p>

<p>
  <b>Title:</b>
  <%= @post.title %>
</p>

<p>
  <b>Content:</b>
  <%= @post.content %>
</p>

<% if signed_in? && super_admin? %>
  <%= link_to 'Edit', edit_post_path(@post) %>
<% end %>

<%= link_to 'Back', posts_path %>
</div>
</div>
